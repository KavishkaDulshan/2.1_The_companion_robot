// --- Setup ---
void setup()
{
	Serial.begin(115200);
	u8g2.begin();
	pinMode(SENSOR_PIN, INPUT);

	if (!mpu.begin())
	{
		Serial.println("MPU6050 not found!");
		while (1)
			delay(10);
	}
}

// --- Main Loop ---
void loop()
{
	unsigned long currentMillis = millis();
	currentTouchState = digitalRead(SENSOR_PIN);

	// --- Intro Animation ---
	if (playIntro)
	{
		if (frame < introallArray_LEN)
		{
			u8g2.clearBuffer();
			u8g2.drawXBMP(0, 0, 128, 64, introallArray[frame]);
			u8g2.sendBuffer();
			frame++;
			delay(frameInterval);
		}
		else
		{
			playIntro = false;
			introPlayed = true;
			frame = 0;
		}
		return;
	}

	// --- Touch Handling ---
	if (currentTouchState == HIGH && lastTouchState == LOW)
	{
		Serial.println("Sensor touched");
		sensorReleased = false;
		touchStartTime = currentMillis;
		isTouching = true;
		touchHoldTriggered = false;

		// Track rapid touches for anger trigger
		if (touchCount == 0)
			firstTouchTime = currentMillis;

		touchCount++;
		if (currentMillis - firstTouchTime > touchBurstWindow)
		{
			touchCount = 1;
			firstTouchTime = currentMillis;
		}

		if (touchCount >= touchThreshold)
		{
			playAnger = true;
			angerReverse = false;
			frame = 0;
			playEpd = false;
			playAwake = false;
			touchCount = 0;
			return;
		}

		if (idleTimeoutTriggered)
		{
			playAwake = true;
			frame = 0;
			idleTimeoutTriggered = false;
			playEpd = false;
			return;
		}
	}
	else if (currentTouchState == LOW && lastTouchState == HIGH)
	{
		Serial.println("Sensor released");
		releaseTime = currentMillis;
		sensorReleased = true;
		isTouching = false;
	}
	lastTouchState = currentTouchState;

	// --- Touch-and-hold check ---
	if (isTouching && !touchHoldTriggered && (currentMillis - touchStartTime >= touchHoldDuration))
	{
		Serial.println("Touch held for 5 seconds");
		touchHoldTriggered = true;
		playEpd = true;
		playAwake = false;
		frame = 0;
		return;
	}

	// --- MPU6050 Data ---
	sensors_event_t a, g, temp;
	mpu.getEvent(&a, &g, &temp);

	float currentAccelMagnitude = sqrt(
		a.acceleration.x * a.acceleration.x +
		a.acceleration.y * a.acceleration.y +
		a.acceleration.z * a.acceleration.z);

	if (!accelInitialized)
	{
		lastAccelMagnitude = currentAccelMagnitude;
		accelInitialized = true;
	}
	else
	{
		float delta = abs(currentAccelMagnitude - lastAccelMagnitude);
		if (delta > spikeThreshold)
		{
			Serial.println("Acceleration spike detected!");
			playAnger = true;
			angerReverse = false;
			frame = 0;

			returnToSleepAfterAnger = playEpd || idleTimeoutTriggered;

			playEpd = false;
			playAwake = false;
			idleTimeoutTriggered = false;

			touchCount = 0;
			return;
		}
		lastAccelMagnitude = currentAccelMagnitude;
	}

	// --- Idle Detection ---
	float yMovementThreshold = 0.2;
	static float lastY = 0;
	static bool yStable = true;
	static unsigned long yStableSince = 0;

	if (abs(a.acceleration.y - lastY) > yMovementThreshold)
	{
		yStable = false;
		yStableSince = 0;
		releaseTime = currentMillis;
	}
	else
	{
		if (!yStable)
		{
			yStable = true;
			yStableSince = currentMillis;
		}
	}
	lastY = a.acceleration.y;

	if (!isTouching && !playEpd && currentTilt == TILT_NONE && !playAwake && !idleTimeoutTriggered &&
		(currentMillis - releaseTime > 60000) && yStable && (currentMillis - yStableSince > 3000))
	{
		playEpd = true;
		idleTimeoutTriggered = true;
		frame = 0;
		return;
	}

	// --- Anger Animation ---
	if (playAnger)
	{
		u8g2.clearBuffer();
		if (!angerReverse)
		{
			if (frame < angerallArray_LEN)
			{
				u8g2.drawXBMP(0, 0, 128, 64, angerallArray[frame]);
				u8g2.sendBuffer();
				frame++;
			}
			else
			{
				angerReverse = true;
				frame = angerallArray_LEN - 1;
			}
		}
		else
		{
			if (frame >= 0)
			{
				u8g2.drawXBMP(0, 0, 128, 64, angerallArray[frame]);
				u8g2.sendBuffer();
				frame--;
			}
			else
			{
				playAnger = false;
				angerReverse = false;

				if (returnToSleepAfterAnger)
				{
					playEpd = false;
					idleTimeoutTriggered = true; // keep it true
					u8g2.clearBuffer();
					u8g2.drawXBMP(0, 0, 128, 64, epd_bitmap_allArray[epd_bitmap_allArray_LEN - 1]);
					u8g2.sendBuffer();
					frame = 0;
				}
				else
				{
					frame = 0; // return to idle blinking
				}
				returnToSleepAfterAnger = false;
			}
		}
		delay(frameInterval);
		return;
	}

	// --- Epd (Sleep) Animation ---
	if (playEpd)
	{
		if (frame < epd_bitmap_allArray_LEN)
		{
			u8g2.clearBuffer();
			u8g2.drawXBMP(0, 0, 128, 64, epd_bitmap_allArray[frame]);
			u8g2.sendBuffer();
			frame++;
			delay(40);
		}
		else
		{
			if (touchHoldTriggered)
			{
				playEpd = false;
				playAwake = true;
				frame = 0;
			}
			else
			{
				u8g2.clearBuffer();
				u8g2.drawXBMP(0, 0, 128, 64, epd_bitmap_allArray[epd_bitmap_allArray_LEN - 1]);
				u8g2.sendBuffer();
			}
		}
		return;
	}

	// --- Awake Animation ---
	if (playAwake)
	{
		if (frame < awakeallArray_LEN)
		{
			u8g2.clearBuffer();
			u8g2.drawXBMP(0, 0, 128, 64, awakeallArray[frame]);
			u8g2.sendBuffer();
			frame++;
			delay(40);
		}
		else
		{
			playAwake = false;
			touchHoldTriggered = false;
			frame = 0;
		}
		return;
	}

	// --- Tilt Detection ---
	if (a.acceleration.y < -3)
	{
		if (currentTilt != TILT_LEFT)
		{
			lastTilt = currentTilt;
			currentTilt = TILT_LEFT;
			frame = 0;
			lookAnimDone = false;
			reverseTransition = false;
		}
	}
	else if (a.acceleration.y > 3)
	{
		if (currentTilt != TILT_RIGHT)
		{
			lastTilt = currentTilt;
			currentTilt = TILT_RIGHT;
			frame = 0;
			lookAnimDone = false;
			reverseTransition = false;
		}
	}
	else
	{
		if (currentTilt != TILT_NONE)
		{
			lastTilt = currentTilt;
			currentTilt = TILT_NONE;
			frame = (lastTilt == TILT_LEFT) ? lookleftallArray_LEN - 1 : lookrightallArray_LEN - 1;
			reverseTransition = true;
		}
	}

	// --- Animation Update ---
	if (currentMillis - previousFrameTime >= frameInterval)
	{
		previousFrameTime = currentMillis;
		u8g2.clearBuffer();

		if (reverseTransition)
		{
			if (lastTilt == TILT_LEFT && frame >= 0)
			{
				u8g2.drawXBMP(0, 0, 128, 64, lookleftallArray[frame]);
				frame--;
				if (frame < 0)
				{
					reverseTransition = false;
					frame = 0;
				}
			}
			else if (lastTilt == TILT_RIGHT && frame >= 0)
			{
				u8g2.drawXBMP(0, 0, 128, 64, lookrightallArray[frame]);
				frame--;
				if (frame < 0)
				{
					reverseTransition = false;
					frame = 0;
				}
			}
		}
		else
		{
			switch (currentTilt)
			{
			case TILT_LEFT:
				if (!lookAnimDone && frame < lookleftallArray_LEN)
				{
					u8g2.drawXBMP(0, 0, 128, 64, lookleftallArray[frame]);
					frame++;
					if (frame >= lookleftallArray_LEN)
					{
						lookAnimDone = true;
						frame = lookleftallArray_LEN - 1;
					}
				}
				else
				{
					u8g2.drawXBMP(0, 0, 128, 64, lookleftallArray[frame]);
				}
				break;

			case TILT_RIGHT:
				if (!lookAnimDone && frame < lookrightallArray_LEN)
				{
					u8g2.drawXBMP(0, 0, 128, 64, lookrightallArray[frame]);
					frame++;
					if (frame >= lookrightallArray_LEN)
					{
						lookAnimDone = true;
						frame = lookrightallArray_LEN - 1;
					}
				}
				else
				{
					u8g2.drawXBMP(0, 0, 128, 64, lookrightallArray[frame]);
				}
				break;

			case TILT_NONE:
				if (!blinkPaused)
				{
					u8g2.drawXBMP(0, 0, 128, 64, blink_allArray[frame]);
					frame++;
					delay(10);
					if (frame >= blink_allArray_LEN)
					{
						blinkPaused = true;
						blinkPauseStartTime = currentMillis;
						frame = blink_allArray_LEN - 1;
					}
				}
				else
				{
					u8g2.drawXBMP(0, 0, 128, 64, blink_allArray[frame]);
					if (currentMillis - blinkPauseStartTime >= 5000)
					{
						blinkPaused = false;
						frame = 0;
					}
				}
				break;
			}
		}
		u8g2.sendBuffer();
	}
}