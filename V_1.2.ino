	0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00
};

// Array of all bitmaps for convenience. (Total bytes used to store images in PROGMEM = 6240)
const int lookrightallArray_LEN = 6;
const unsigned char* lookrightallArray[6] = {
	lookright0,
	lookright1,
	lookright2,
	lookright3,
	lookright4,
	lookright5
};




void setup(void) {
  u8g2.begin();
  Serial.begin(115200);
  pinMode(SENSOR_PIN, INPUT);
  if (!mpu.begin()) {
    Serial.println("MPU6050 not found!");
    while (1) delay(10);
  }
}

void loop() {
  // Read sensor data from MPU6050
  sensors_event_t a, g, temp;
  mpu.getEvent(&a, &g, &temp);
  unsigned long currentMillis = millis();

  // Detect tilt state from X-axis
  if (a.acceleration.y < -3) {
    if (currentTilt != TILT_LEFT) {
      currentTilt = TILT_LEFT;
      frame = 0;
      lookAnimDone = false;
    }
  } else if (a.acceleration.y > 3) {
    if (currentTilt != TILT_RIGHT) {
      currentTilt = TILT_RIGHT;
      frame = 0;
      lookAnimDone = false;
    }
  } else {
    if (currentTilt != TILT_NONE) {
      currentTilt = TILT_NONE;
      frame = 0;
    }
  }

  // Animation frame update timing
  if (currentMillis - previousFrameTime >= frameInterval) {
    previousFrameTime = currentMillis;
    u8g2.clearBuffer();

    switch (currentTilt) {
      case TILT_LEFT:
        if (!lookAnimDone && frame < lookleftallArray_LEN) {
          u8g2.drawXBMP(0, 0, 128, 64, lookleftallArray[frame]);
          frame++;
          if (frame >= lookleftallArray_LEN) {
            lookAnimDone = true;
            frame = lookleftallArray_LEN - 1;  // Hold final frame
          }
        } else {
          u8g2.drawXBMP(0, 0, 128, 64, lookleftallArray[frame]); // Hold last
        }
        break;

      case TILT_RIGHT:
        if (!lookAnimDone && frame < lookrightallArray_LEN) {
          u8g2.drawXBMP(0, 0, 128, 64, lookrightallArray[frame]);
          frame++;
          if (frame >= lookrightallArray_LEN) {
            lookAnimDone = true;
            frame = lookrightallArray_LEN - 1;  // Hold final frame
          }
        } else {
          u8g2.drawXBMP(0, 0, 128, 64, lookrightallArray[frame]); // Hold last
        }
        break;

      case TILT_NONE:
        // Default blinking animation
        u8g2.drawXBMP(0, 0, 128, 64, blink_allArray[frame]);
        frame++;
        if (frame >= blink_allArray_LEN) {
          frame = 0;
        }
        break;
    }

    u8g2.sendBuffer();
  }
}
